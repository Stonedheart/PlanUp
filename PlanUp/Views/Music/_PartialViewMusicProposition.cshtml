@model PlanUp.Models.MusicModel

<div class="container">
    <div class="row">
    @if (Model.Playlists != null)
    {
        foreach (var playlist in Model.Playlists)
        {
            <div class="col-sm-4">
                <iframe width="420" height="315" src="playlist.Etag"></iframe>
            </div>
        }
    }
    </div>
</div>
