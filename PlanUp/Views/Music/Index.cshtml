@model PlanUp.Models.Song[]

@{
    ViewBag.Title = "Music";
}

<h2>Music</h2>

<table class="table">
    
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Title
                    <iframe width="420" height="315"
                            src="@item.Etag">
                    </iframe>
                </td>
                <td>
                    @item.Description
                </td>
            </tr>
        }
    }
</table>
